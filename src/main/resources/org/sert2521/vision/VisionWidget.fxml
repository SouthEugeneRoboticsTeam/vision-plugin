           fx:controller="org.sert2521.vision.VisionWidget"
           fx:id="root"
           minWidth="256" minHeight="160">
    <padding>
        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0"/>
    </padding>
    <Canvas fx:id="canvas" StackPane.alignment="CENTER"/>
</StackPane>
